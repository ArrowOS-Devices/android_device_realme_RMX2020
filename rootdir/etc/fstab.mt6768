/dev/block/platform/bootdevice/by-name/userdata         /data                   f2fs     noatime,nosuid,nodev,discard,noflush_merge,fsync_mode=nobarrier,reserve_root=134217,resgid=1065 wait,check,formattable,reservedsize=128m,latemount,fileencryption=aes-256-xts,readahead_size_kb=128
